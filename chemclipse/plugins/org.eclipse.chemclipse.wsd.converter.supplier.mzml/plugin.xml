<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.chemclipse.wsd.converter.chromatogramSupplier">
      <ChromatogramSupplier
            description="Reads mzML HPLC-PDA Chromatograms."
            fileExtension=".mzML"
            filterName="PDA Chromatogram (*.mzML)"
            id="org.eclipse.chemclipse.wsd.converter.supplier.mzml"
            exportConverter="org.eclipse.chemclipse.wsd.converter.supplier.mzml.converter.ChromatogramExportConverter"
            importConverter="org.eclipse.chemclipse.wsd.converter.supplier.mzml.converter.ChromatogramImportConverter"
            importMagicNumberMatcher="org.eclipse.chemclipse.xxd.converter.supplier.mzml.converter.MagicNumberMatcher"
            importContentMatcher="org.eclipse.chemclipse.wsd.converter.supplier.mzml.converter.FileContentMatcher"
            isExportable="true"
            isImportable="true">
      </ChromatogramSupplier>
   </extension>
</plugin>