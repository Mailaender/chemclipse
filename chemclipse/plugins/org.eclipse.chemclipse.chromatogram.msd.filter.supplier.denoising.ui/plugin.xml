         <adapter
               type="org.eclipse.jface.viewers.ITableLabelProvider">
         </adapter>
         <adapter
               type="org.eclipse.chemclipse.support.ui.swt.columns.ColumnDefinitionProvider">
         </adapter>
         <adapter
               type="org.eclipse.jface.viewers.ISelectionChangedListener">
         </adapter>
      </factory>
